section(data-ng-controller="ArtikelCtrl" data-ng-init="find()")
  h1.page-header Artikel Liste

  #alarm.alert.alert-danger(ng-if="err.status=='500'" role="alert") <b>{{ err.status }}</b> {{ err.data.data.message }}

  table.table.table-hover.table-condensed
    thead
      tr
        th #
        th Artikel
        th Baureihe
        th Art
    tbody
      tr(data-ng-repeat="art in artikel")
        td(data-ng-bind="art.Material")
        td(data-ng-bind="art.Materialkurztext")
        td(data-ng-bind="art.Baureihe")
        td(data-ng-bind="art.Art")
